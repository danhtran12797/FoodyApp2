<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/background_set_up_now"
    android:gravity="center"
    tools:context=".feature.dashboard.DashboardFragment">

    <com.nex3z.flowlayout.FlowLayout
        android:id="@+id/flow_layout"
        app:flRtl="true"
        app:flChildSpacing="@dimen/_8"
        app:flRowSpacing="@dimen/_8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/chip1"
            style="@style/Widget.MaterialComponents.Chip.Action"
            android:background="@drawable/background_chip"
            android:paddingHorizontal="@dimen/_10"
            android:gravity="center"
            android:minHeight="@dimen/_32"
            android:textColor="@color/thumb_scroll_bar"
            android:textSize="@dimen/text_size_13"
            android:fontFamily="@font/roboto_regular"
            android:text="@string/jurisdictions_free"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/chip2"
            style="@style/Widget.MaterialComponents.Chip.Action"
            android:textColor="@color/thumb_scroll_bar"
            android:textSize="@dimen/text_size_13"
            android:fontFamily="@font/roboto_regular"
            android:background="@drawable/background_chip"
            android:paddingHorizontal="@dimen/_10"
            android:gravity="center"
            android:minHeight="@dimen/_32"
            android:text="@string/service_fee"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/chip3"
            style="@style/Widget.MaterialComponents.Chip.Action"
            android:textColor="@color/thumb_scroll_bar"
            android:textSize="@dimen/text_size_13"
            android:fontFamily="@font/roboto_regular"
            android:background="@drawable/background_chip"
            android:paddingHorizontal="@dimen/_10"
            android:gravity="center"
            android:minHeight="@dimen/_32"
            android:text="@string/open_account"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/chip4"
            style="@style/Widget.MaterialComponents.Chip.Action"
            android:textColor="@color/thumb_scroll_bar"
            android:textSize="@dimen/text_size_13"
            android:fontFamily="@font/roboto_regular"
            android:background="@drawable/background_chip"
            android:paddingHorizontal="@dimen/_10"
            android:gravity="center"
            android:minHeight="@dimen/_32"
            android:text="@string/account_open_consultancy"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/chip5"
            style="@style/Widget.MaterialComponents.Chip.Action"
            android:textColor="@color/thumb_scroll_bar"
            android:textSize="@dimen/text_size_13"
            android:fontFamily="@font/roboto_regular"
            android:background="@drawable/background_chip"
            android:paddingHorizontal="@dimen/_10"
            android:gravity="center"
            android:minHeight="@dimen/_32"
            android:text="@string/back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/chip6"
            style="@style/Widget.MaterialComponents.Chip.Action"
            android:textColor="@color/thumb_scroll_bar"
            android:textSize="@dimen/text_size_13"
            android:fontFamily="@font/roboto_regular"
            android:background="@drawable/background_chip"
            android:paddingHorizontal="@dimen/_10"
            android:gravity="center"
            android:minHeight="@dimen/_32"
            android:text="@string/service_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/chip7"
            style="@style/Widget.MaterialComponents.Chip.Action"
            android:textColor="@color/thumb_scroll_bar"
            android:textSize="@dimen/text_size_13"
            android:fontFamily="@font/roboto_regular"
            android:background="@drawable/background_chip"
            android:paddingHorizontal="@dimen/_10"
            android:gravity="center"
            android:minHeight="@dimen/_32"
            android:text="@string/hatchworks_company"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/chip8"
            style="@style/Widget.MaterialComponents.Chip.Action"
            android:textColor="@color/thumb_scroll_bar"
            android:textSize="@dimen/text_size_13"
            android:fontFamily="@font/roboto_regular"
            android:background="@drawable/background_chip"
            android:paddingHorizontal="@dimen/_10"
            android:gravity="center"
            android:minHeight="@dimen/_32"
            android:text="@string/contact_details_shipping_address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </com.nex3z.flowlayout.FlowLayout>


